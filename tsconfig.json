{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "baseUrl": "./",
    "lib": ["es2015", "dom"],
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "xm-tabs": [
        "src"
      ],
      "xm-tabs/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*"
  ]
}